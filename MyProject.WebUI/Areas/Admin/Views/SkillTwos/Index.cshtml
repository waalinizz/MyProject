@model MyProject.WebUI.Areas.Admin.Models.SkillTwoModel.SkillTwoListViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Yetenek</th>
            <th scope="col">Yetenek Yüzdesi</th>
            <th scope="col">Sil</th>
            <th scope="col">Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.SkillTwos)
        {
            <tr>
                <th scope="row">@item.SkillTwoId</th>
                <td>@item.SkillItem</td>
                <td>@item.SkillItemPercent</td>
                <td><a href="/Admin/SkillTwos/Delete/@item.SkillTwoId" class="btn btn-danger">Sil</a></td>
                <td><a href="/Admin/SkillTwos/Update/@item.SkillTwoId" class="btn btn-warning">Güncelle</a></td>
            </tr>
        }
    </tbody>
</table>
<a href="/Admin/SkillTwos/Add" class="btn btn-success">Ekle</a>


